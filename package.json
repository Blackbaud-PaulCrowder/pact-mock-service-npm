    "url": "https://github.com/bethesque/pact-mock_service/issues"
  },
  "homepage": "https://github.com/bethesque/pact-mock-service-npm",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.1",
